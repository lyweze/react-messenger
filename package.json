{
	"name": "react-messenger",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"homepage": "https://lyweze.github.io/react-messenger/",
	"scripts": {
		"dev": "concurrently \"npm run dev:server\" \"npm run dev:client\" --kill-others",
		"dev:server": "cd server && npm run dev",
		"dev:client": "cd client && npm run dev",
		"build": "vite build",
		"lint": "eslint .",
		"preview": "vite preview",
		"predeploy": "npm run build",
		"deploy": "gh-pages -d dist"
	},
	"dependencies": {
		"react": "^19.1.0",
		"react-dom": "^19.1.0",
		"react-router-dom": "^7.6.2"
	},
	"devDependencies": {
		"@eslint/js": "^9.25.0",
		"@types/react": "^19.1.2",
		"@types/react-dom": "^19.1.2",
		"@vitejs/plugin-react": "^4.4.1",
		"concurrently": "^9.1.2",
		"eslint": "^9.25.0",
		"eslint-plugin-react-hooks": "^5.2.0",
		"eslint-plugin-react-refresh": "^0.4.19",
		"globals": "^16.0.0",
		"vite": "^6.3.5"
	}
}
